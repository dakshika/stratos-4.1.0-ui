{
    "type":"object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "id": "root",
    "required":true,
    "properties":{
        "id": {
            "type":"string",
            "title": "Partition ID",
            "name": "Partition ID",
            "description": "ID for partition",
            "id": "root/id",
            "default": "P1",
            "required":true
        },
        "provider": {
            "type":"string",
            "id": "root/provider",
            "default": "ec2",
            "required":true
        },
        "property": {
            "type":"array",
            "id": "root/property",
            "required":true,
            "items":[
                {
                    "type":"object",
                    "id": "root/property/0",
                    "required":true,
                    "properties":{
                        "name": {
                            "type":"string",
                            "id": "root/property/0/name",
                            "default": "region",
                            "required":true
                        },
                        "value": {
                            "type":"string",
                            "id": "root/property/0/value",
                            "default": "ap-southeast-1",
                            "required":true
                        }
                    }
                },
                {
                    "type":"object",
                    "id": "root/property/1",
                    "required":true,
                    "properties":{
                        "name": {
                            "type":"string",
                            "id": "root/property/1/name",
                            "default": "zone",
                            "required":true
                        },
                        "value": {
                            "type":"string",
                            "id": "root/property/1/value",
                            "default": "ap-southeast-1a",
                            "required":true
                        }
                    }
                }
            ]
        }
    }
}
